{
  "name": "@digime/digime-sdk-nodejs",
  "version": "10.1.1",
  "description": "Node.JS SDK for digi.me",
  "main": "dist/main/index.js",
  "types": "dist/main/index.d.ts",
  "module": "dist/module/index.js",
  "scripts": {
    "build": "npm run clear-dist && npm run compile",
    "compile": "npm run compile:main && npm run compile:module",
    "compile:main": "./node_modules/.bin/tsc -p tsconfig.dist.json",
    "compile:module": "./node_modules/.bin/tsc -p tsconfig.module.json",
    "clear-dist": "rimraf ./dist",
    "lint": "eslint . --max-warnings=0",
    "lint-fix": "eslint --fix . --ext .js,.ts",
    "test": "./node_modules/.bin/jest --coverage --config=jest-config.json",
    "version": "ts-node ./tools/update-sdk-version.ts",
    "generate-docs": "./node_modules/.bin/typedoc",
    "preversion": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digime/digime-sdk-nodejs.git"
  },
  "author": "digi.me Limited",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/digime/digime-sdk-nodejs/issues"
  },
  "homepage": "https://github.com/digime/digime-sdk-nodejs#readme",
  "devDependencies": {
    "@knodes/typedoc-plugin-pages": "~0.23.1",
    "@types/jest": "~29.2.4",
    "@types/jsonwebtoken": "~9.0.0",
    "@types/lodash": "~4.14.191",
    "@types/lodash.get": "~4.4.7",
    "@types/lodash.isfunction": "~3.0.7",
    "@types/lodash.isplainobject": "~4.0.7",
    "@types/lodash.isstring": "~4.0.7",
    "@types/lodash.memoize": "~4.1.7",
    "@types/lodash.omit": "~4.5.7",
    "@types/node": "~14.18.36",
    "@types/sprintf-js": "~1.1.2",
    "@typescript-eslint/eslint-plugin": "~5.47.1",
    "@typescript-eslint/parser": "~5.47.1",
    "cpr": "~3.0.1",
    "eslint": "~8.30.0",
    "eslint-config-prettier": "~8.5.0",
    "eslint-plugin-header": "~3.1.1",
    "eslint-plugin-prettier": "~4.2.1",
    "eslint-plugin-unicorn": "~45.0.2",
    "jest": "~29.3.1",
    "jest-circus": "~29.3.1",
    "nock": "~13.2.9",
    "prettier": "~2.8.1",
    "rimraf": "~3.0.2",
    "ts-jest": "~29.0.3",
    "ts-node": "~10.9.1",
    "typedoc": "~0.23.23",
    "typedoc-plugin-missing-exports": "~1.0.0",
    "typescript": "~4.9.4"
  },
  "dependencies": {
    "@types/node-rsa": "~1.1.1",
    "@types/urijs": "~1.19.19",
    "base64url": "~3.0.1",
    "form-data": "~4.0.0",
    "fp-ts": "~2.13.1",
    "got": "~11.8.6",
    "io-ts": "~2.2.20",
    "jsonwebtoken": "~9.0.0",
    "lodash.get": "~4.4.2",
    "lodash.isfunction": "~3.0.9",
    "lodash.isplainobject": "~4.0.6",
    "lodash.isstring": "~4.0.1",
    "lodash.memoize": "~4.1.2",
    "lodash.omit": "~4.5.0",
    "node-rsa": "~1.1.1",
    "sprintf-js": "~1.1.2",
    "urijs": "~1.19.11"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
